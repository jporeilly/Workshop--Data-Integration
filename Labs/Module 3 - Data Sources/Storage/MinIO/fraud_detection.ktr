  <step>
    <name>Metrics + Flags</name>
    <type>Formula</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <formula>
      <field_name>days_since_account_open</field_name>
      <formula_string>ABS(DAYS([transaction_date]; [open_date]))</formula_string>
      <value_type>Integer</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field/>
    </formula>
    <formula>
      <field_name>balance_after_transaction</field_name>
      <formula_string> IF([account_type]="CREDIT"; [balance] - [amount]; [balance] - [amount])</formula_string>
      <value_type>Number</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field/>
    </formula>
    <formula>
      <field_name>credit_utilization</field_name>
      <formula_string>IF([credit_limit] > 0; ([amount] / [credit_limit]) * 100; 0)</formula_string>
      <value_type>Integer</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field/>
    </formula>
    <formula>
      <field_name>amount_usd</field_name>
      <formula_string> IF([currency]="USD"; [amount]; IF([currency]="EUR"; [amount]*1.08; IF([currency]="GBP"; [amount]*1.27; [amount])))</formula_string>
      <value_type>Number</value_type>
      <value_length>15</value_length>
      <value_precision>2</value_precision>
      <replace_field/>
    </formula>
    <formula>
      <field_name>high_amount_flag</field_name>
      <formula_string>IF([amount] > 1000; 1; 0)</formula_string>
      <value_type>Integer</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field/>
    </formula>
    <formula>
      <field_name>overlimit_flag</field_name>
      <formula_string>IF(AND([account_type]="CREDIT"; [balance_after_transaction] &lt; 0); 1; 0)</formula_string>
      <value_type>Integer</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field/>
    </formula>
    <formula>
      <field_name>high_risk_merchant_flag</field_name>
      <formula_string>IF([risk_level]="HIGH"; 1; 0)</formula_string>
      <value_type>Integer</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field/>
    </formula>
    <formula>
      <field_name>velocity_flag</field_name>
      <formula_string>IF([status]="DECLINED"; 1; 0)</formula_string>
      <value_type>Integer</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field/>
    </formula>
    <formula>
      <field_name>new_account_flag</field_name>
      <formula_string>IF([days_since_account_open] &lt; 30; 1; 0)</formula_string>
      <value_type>Integer</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field/>
    </formula>
    <formula>
      <field_name>high_utilization_flag</field_name>
      <formula_string>IF([credit_utilization] > 80; 1; 0)</formula_string>
      <value_type>Integer</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field/>
    </formula>
