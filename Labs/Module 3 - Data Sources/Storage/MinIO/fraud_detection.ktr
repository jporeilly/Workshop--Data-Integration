    <formula>
      <field_name>fraud_risk_score</field_name>
      <formula_string>([high_amount_flag] * 20) +
([overlimit_flag] * 30) +
([high_risk_merchant_flag] * 25) +
([velocity_flag] * 15) +
([new_account_flag] * 5) +
([high_utilization_flag] * 15)</formula_string>
      <value_type>Integer</value_type>
      <value_length>-1</value_length>
