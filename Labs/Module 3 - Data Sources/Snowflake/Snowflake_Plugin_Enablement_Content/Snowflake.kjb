    </entry>
    <entry>
      <name>Create Snowflake View</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>USE PENTAHO_DB;

DROP VIEW  IF EXISTS SALES_CUSTOMER_VIEW;

CREATE VIEW SALES_CUSTOMER_VIEW as
select "compDim"."COMPANY_NAME", year("DATE") as YEAR, month("DATE") as MONTH, "sf"."NET_SALES_TOTAL" as NET_SALES_TOTAL, "sf"."NET_SALES_WITH_TAX" AS NET_SALES_WITH_TAX from "PENTAHO_DB"."PUBLIC"."SALES_FACT" as "sf" 
inner join "PENTAHO_DB"."PUBLIC"."DATE_DIM" as "dateDim" on "dateDim"."DATE_FK" = "sf"."DATE_FK"
inner join "PENTAHO_DB"."PUBLIC"."COMPANY_DIM" as "compDim" on "compDim"."COMPANY_FK" = "sf"."COMPANY_FK"

</sql>
