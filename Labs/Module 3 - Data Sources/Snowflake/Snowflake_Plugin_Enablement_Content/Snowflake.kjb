    <entry>
      <name>Create Snowflake Fact-Dimension Tables </name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>use database Pentaho_DB;

drop table if exists public.sales_fact;


create table sales_fact (date_fk number, company_fk number, net_sales_total number(5,2),net_sales_with_tax  number(5,2) );



drop table if exists public.date_dim;
 
create table date_dim (date_fk NUMBER, date DATE);



drop table if exists public.company_dim;
 
create table company_dim (company_fk NUMBER, company_name VARCHAR(50));




</sql>
